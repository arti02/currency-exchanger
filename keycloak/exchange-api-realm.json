{
  "realm": "exchange-api",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": false,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "clients": [
    {
      "clientId": "exchange-api-client",
      "enabled": true,
      "publicClient": true,
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "directAccessGrantsEnabled": true
    }
  ],
  "users": [
    {
      "username": "trader",
      "enabled": true,
      "email": "trader@example.com",
      "emailVerified": true,
      "firstName": "John",
      "lastName": "Trader",
      "credentials": [
        {
          "type": "password",
          "value": "password123",
          "temporary": false
        }
      ],
      "realmRoles": ["TRADER"]
    },
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@example.com",
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": ["ADMIN", "TRADER"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "TRADER",
        "description": "Trader role for currency exchange"
      },
      {
        "name": "ADMIN",
        "description": "Administrator role"
      }
    ]
  }
}
